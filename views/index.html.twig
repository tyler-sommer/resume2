<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Tyler Sommer - Resume</title>
  <meta name="description" content="Tyler Sommer's Online Resume">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="author" content="Tyler Sommer">

  <link rel="icon" type="image/x-icon" href="favicon.ico" />
  
  <link href="//cdn.jsdelivr.net/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet" type="text/css">
  <link href="//cdn.jsdelivr.net/fontawesome/4.2.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
  <link href="//fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet" type="text/css">

  <link rel="stylesheet" href="{{  asset('css/app.min.css') }}">
  
  <!--[if lt IE 9]>
  <script src="//cdn.jsdelivr.net/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="//cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script>
  <!--[endif]-->
</head>
<body>
  <div class="navbar navbar-default navbar-static-top" role="navigation">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>
      <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav" id="main-menu">
          <li><a href="#welcome">Home</a></li>
          <li><a href="#profile">Profile</a></li>
          <li><a href="#experience">Experience</a></li>
          <li><a href="#portfolio">Portfolio</a></li>
          <li><a href="#contact">Contact</a></li>
        </ul>
      </div>
    </div>
  </div>
 
  <section id="welcome" class="page-welcome">
    <div>
      <ul class="slider-controls">
        <li><a id="vegas-next" class="next" href="#"></a></li>
        <li><a id="vegas-prev" class="prev" href="#"></a></li>
      </ul>
    </div>
    <div class="container">
      <div class="row">
        <header class="centered">
          <h1>{{ resume.name }}</h1>
          <p>{{ resume.title }}</p>
        </header>
        <div class="social-icons">
          <a href="https://github.com/tyler-sommer" class="btn btn-round btn-clear btn-github"><i class="fa fa-github"></i></a>
          <a href="https://twitter.com/syncreticidiot" class="btn btn-round btn-clear btn-twitter"><i class="fa fa-twitter"></i></a>
          <a href="https://www.facebook.com/tyler.sommer" class="btn btn-round btn-clear btn-facebook"><i class="fa fa-facebook"></i></a>
          <a href="https://www.instagram.com/syncreticidiot" class="btn btn-round btn-clear btn-instagram"><i class="fa fa-instagram"></i></a>
          <a href="https://www.linkedin.com/in/tylersommer/" class="btn btn-round btn-clear btn-linkedin"><i class="fa fa-linkedin-square"></i></a>
          <a href="https://plus.google.com/+TylerSommer" class="btn btn-round btn-clear btn-google"><i class="fa fa-google-plus"></i></a>
        </div>
      </div>
    </div>
  </section>

  <section id="profile" class="page-profile">
    <div class="container">
      <header class="section-header">
        <h2 class="section-title">Personal Profile</h2>
        <div class="spacer"></div>
        <p class="section-subtitle">"Leet Geek" from Salt Lake City.</p>
      </header>
      <div class="row">
        <div class="col-md-3">
          <div class="profile"><img src="{{ asset('img/profile.jpeg') }}" alt="profile"/></div>
        </div>
        <div class="col-md-9">
          {% for line in resume.profile %}
          <p>{{ line }}</p>
          {% endfor %}
        </div>
      </div>
    </div> 
  </section>

  <section id="experience" class="page-experience">
    <div class="container">
      <header class="section-header">
        <h2 class="section-title"><span>Experience</span></h2>
        <div class="spacer"></div>
        <p class="section-subtitle">Full-stack developer. Backend specialist.</p>
      </header>
      <div class="row">
        {% for position in resume.experience %}
          {% if loop.index0 < 3 %}
        <div class="col-md-4">
          <article class="experience">
            <header>
              <h3>{{ position.company }}</h3>
              <p>{{ position.title }} / {{ position.location }} / {{ position.from }} - {{ position.to }}</p>
            </header>
            {% for line in position.description %}
            <p>{{ line }}</p>
            {% endfor %}
          </article>
        </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </section>

  <section id="portfolio" class="page-portfolio">
    <div class="container">
      <div class="row">
        <header class="section-header">
          <h2 class="section-title"><span>Portfolio</span></h2>
          <div class="spacer"></div>
          <p class="section-subtitle">Creating, improving, maintaining.</p>
        </header>
        
        <div id="grid-controls-wrapper">
          <ul class="nav nav-pills center-pills grid-controls">
            <li class="active filter"><a href="javascript:void(0)" data-filter="*">All</a></li>
            <li class="filter"><a href="javascript:void(0)" data-filter=".backend">Backend</a></li>           
            <li class="filter"><a href="javascript:void(0)" data-filter=".open-source">Open Source</a></li>
            <li class="filter"><a href="javascript:void(0)" data-filter=".mobile">Mobile</a></li>
          </ul>
        </div>
        
        <div class="col-md-8 col-md-offset-2">
          <ul id="grid" class="grid-wrapper">
            <li class="mix backend open-source">
              <a href="{{ asset('img/portfolio/large/portfolio-bigred.png') }}">
                <div class="overlay"><i class="fa fa-search"></i></div>
                <img src="{{ asset('img/portfolio/small/portfolio-bigred-sm.png') }}" alt="Big Red, The Button Does Things~~" title="Big Red, The Button Does Things~~">
              </a> 
            </li>
            <li class="mix open-source">
              <a href="{{ asset('img/portfolio/large/portfolio-squircy.png') }}">
                <div class="overlay"><i class="fa fa-search"></i></div>
                <img src="{{ asset('img/portfolio/small/portfolio-squircy-sm.png') }}" alt="squIRCy2 IRC Bot" title="squIRCy2 IRC Bot">
              </a>
            </li>
            <li class="mix backend open-source">
              <a href="{{ asset('img/portfolio/large/portfolio-packages.png') }}">
                <div class="overlay"><i class="fa fa-search"></i></div>
                <img src="{{ asset('img/portfolio/small/portfolio-packages-sm.png') }}" alt="Packages - Composer Repository Management" title="Packages - Composer Repository Management">
              </a>
            </li>
            <li class="mix mobile">
              <a href="{{ asset('img/portfolio/large/portfolio-epk-ios.png') }}">
                <div class="overlay"><i class="fa fa-search"></i></div>
                <img src="{{ asset('img/portfolio/small/portfolio-epk-ios-sm.png') }}" alt="Easy Payment Kiosk - iOS App" title="Easy Payment Kiosk - iOS App">
              </a>
            </li>
            <li class="mix backend">
              <a href="{{ asset('img/portfolio/large/portfolio-epk-web.png') }}">
                <div class="overlay"><i class="fa fa-search"></i></div>
                <img src="{{ asset('img/portfolio/small/portfolio-epk-web-sm.png') }}" alt="Easy Payment Kiosk - Back office management system" title="Back office management system">
              </a>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>
  
  <section id="contact" class="page-contact">
    <div class="container">
      <header class="section-header">
        <h2 class="section-title"><span>Contact</span></h2>
        <div class="spacer"></div>
        <p class="section-subtitle">Get in touch.</p>
      </header>
      <div class="row">
        <div class="col-sm-4 col-sm-offset-4 contact-info">
          <p><i class="fa fa-fw fa-envelope-o"></i> {{ resume.email }}</p>
          <p><i class="fa fa-fw fa-phone"></i> {{ resume.phone }}</p>
        </div>
      </div>
    </div>
  </section>
  
  <section id="footer" class="page-footer">
    <div class="container">
      <p class="cp">&copy; {{ 'now'|date('Y') }} Tyler Sommer. <a href="{{ path('print') }}">View print version</a>. <a href="https://tylersommer.com">Visit my blog</a>!</p>
    </div>
  </section>

  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-55684658-2', 'auto');
    ga('send', 'pageview');

  </script>
  <script type="text/javascript" src="//cdn.jsdelivr.net/jquery/1.11.1/jquery.min.js"></script>
  <script type="text/javascript" src="//cdn.jsdelivr.net/bootstrap/3.0.3/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="{{ asset('js/app.min.js') }}"></script>
</body>
</html>
